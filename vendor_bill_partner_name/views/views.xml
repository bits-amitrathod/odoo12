        <record id="invoice_supplier_form_partner_name" model="ir.ui.view">
            <field name="name">account.invoice.supplier.form</field>
            <field name="model">account.move</field>
            <field name="inherit_id" ref="account.view_move_form"></field>
            <field name="arch" type="xml">
                <xpath expr="//group/group/field[@name='partner_id']" position="replace">
<!--                  <field string="Vendor" name="partner_id"
                              context="{'default_customer': 0, 'search_default_supplier': 1, 'default_supplier': 1, 'default_company_type': 'company','vendor_bill_partner_name_display_name': True}"
                              domain="[('supplier_rank', '>', 0)]"/>-->

                    <!--context="{
