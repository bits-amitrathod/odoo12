    <!-- add some info on product detials page  checked-->
    <template id="sps_product_page_inherit" inherit_id="website_sale.product" name="SPS Product Page Inherit" priority="10">

        <xpath expr="//p[@t-field='product.description_sale']" position="replace">
             <!-- Manufacturer name -->
             <p class="brand-code">
                <a t-att-href="keep('/shop/brands', brand=product.product_brand_id.id)" t-field="product.product_brand_id.name" class="linkCstm"></a>
                <!-- SKU -->
                <span t-field="product.default_code"></span>
            </p>

        </xpath>
        <xpath expr="//div[@id='o_wsale_cta_wrapper']" position="before">
            <div class="w-100">
                <t t-call="website_sales.category_vender"/>
                <t t-call="website_sales.productExpirationDate"/>
            </div>
        </xpath>
        <t t-call="website_sale.product_price" position="replace">
            <t t-if="combination_info['price'] &gt; 0">
                <t t-call="website_sale.product_price"/>
            </t>
            <t t-else="">
                <p class="call-for">Call for Pricing 412-564-1280</p>
            </t>
        </t>
        <xpath expr="//div[@id='o_wsale_cta_wrapper']" position="replace">

            <div id="o_wsale_cta_wrapper" class="d-flex flex-wrap align-items-center">
                <t t-set="hasQuantities" t-value="false"/>
                <t t-set="hasBuyNow" t-value="false"/>
                <t t-set="ctaSizeBig" t-value="not hasQuantities or not hasBuyNow"/>

                <div t-if="product.actual_quantity &gt; 0" t-attf-class="availability availability_message_#{product_template}">
                    <t t-esc="product.actual_quantity"/> item(s) left
                </div>
                <div t-else="" t-attf-class="availability_message_#{product_template} linkCstm mt16">
                    <div style="color: #666;padding-bottom: 5px;">Get notification as soon as the product is available again. </div>
                    <div class="input-group notifcationGroup" style="width: 90%;padding-bottom: 10px;">
                        <input type="text" class="form-control" aria-label="..." t-att-value="userEmail" name="notificationEmail" t-att-data-productid="product.id"/>
                        <div class="input-group-btn">
                            <button type="button" class="btn btn-danger " aria-haspopup="true" id="notifyMe" aria-expanded="false">Notify me </button>
                        </div>
                    </div><!-- /input-group -->
                    <p style="display: none;" class="invalidEmail">
                        <i class="fa fa-exclamation-triangle"></i>
                        Email address is invalid
                    </p>
                    <div class="alert alert-success notifcationAlert" style="display: none;">You
                        will be notified when this is available.
                    </div>

                    <p style="font-size: 16px;">
                        <i class="fa fa-exclamation-triangle"/>
                        Temporarily out of stock
                    </p>
                </div>
                <div id="add_to_cart_wrap" t-attf-class="{{'d-none' if combination_info['prevent_zero_price_sale'] else 'd-inline-flex'}} align-items-center mb-2 me-auto">
                    <a data-animation-selector=".o_wsale_product_images" role="button" id="add_to_cart" t-attf-class="btn btn-info btn-lg mt8 js_check_product a-submit flex-grow-1 #{'btn-lg' if ctaSizeBig else ''}" href="#">
                        <i class="fa fa-shopping-cart me-2"/>
                        <span>Add to Cart</span>
                    </a>
                </div>
                <div id="product_option_block" class="d-flex flex-wrap w-100"/>
            </div>
        </xpath>
    </template>



