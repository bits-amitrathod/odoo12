<?xml version="1.0" encoding="utf-8"?>
<odoo>
     <template id="headerNavbar_template" name="Main layout" inherit_id="website.template_header_default">
        <xpath expr="//div[@id='top_menu_container']"  position="before">
            <div class="top-header collapse navbar-collapse" id="">
                <ul class="contact-detail">
                    <li>
                        <a href="#">Contact Our Sales Team Today: <span>412.564.1280</span></a>
                    </li>
                </ul>
                <form class="navbar-form navbar-right" role="search" t-attf-action="/shop" method="get">
                    <div class="input-group">
                        <input id="SearchBox" type="text" name="search"
                               class="form-control search-input"
                               placeholder="Search" t-att-value="search"/>
                        <div class="input-group-append">
                            <span class="input-group-text py-0"><button type="submit" class="btn btn-link p-0"><i class="icon-search"></i></button></span>
                        </div>
                    </div>
                </form>
            </div>
        </xpath>
    </template>

