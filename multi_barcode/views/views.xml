<odoo>
  <data>
    <!-- explicit list view definition -->
    <record model="ir.ui.view" id="multi_barcode.list">
      <field name="name">multi_barcode list</field>
      <field name="model">multi.barcode</field>
      <field name="arch" type="xml">
        <tree>
          <field name="product_id"/>
          <field name="barcode"/>
        </tree>
      </field>
    </record>

    <!-- actions opening views on models -->
    <record model="ir.actions.act_window" id="multi_barcode.action_window">
      <field name="name">multi_barcode window</field>
      <field name="res_model">multi.barcode</field>
      <field name="view_mode">tree,form</field>
    </record>

