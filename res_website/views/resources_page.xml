<odoo>
    <template id="resouces_page_template" name="Resource page">
        <t name="About Page" t-name="res_website.resource_page">
            <t t-name="website.resource">
                <t t-call="website.layout">
                    <div id="wrap" class="oe_structure oe_empty resource-page">
                        <section class="">
                            <div class="container padding-60-y">
                                 <div class="text-center">
                                     <h1  class="about-header"> Resources</h1>
                                 </div>
                                <div class="section-description text-center">
                                    <p class="f20 ">Below are a compilation of helpful resources from SPS.</p>
                                </div>
                            </div>
                        </section>
                        <section>
                            <div class="container">
                                <div class="col-md-9 border-separator m-auto"></div>
                            </div>
                        </section>
                            <!-- Video Resources  -->
                        <section class="video-block padding-60-y">
                            <div class="container">
                                 <div class="text-center">
                                     <h1  class="resource-section-title"> <span>Video</span> Library</h1>
                                 </div>
                                <div class="row mt-5">
                                    <t t-foreach="video" t-as="v">
                                        <div class="col-md-4 mb-3 px-3 px-md-4">
                                        <div class="resource-box h-100">
                                            <div class="video-section-wrapper m-0 border-0">
                                                <div class="video-wrapper">
                                                    <a href="javascript:void(0)"  data-bs-toggle="modal" t-att-data-bs-target="'#a'+ str(v.id)"><span class="sr-only">play</span></a>
                                                    <iframe width="100%" height="200" t-att-src="v.url" frameborder="0" allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture" allowfullscreen=""></iframe>
                                                </div>
                                            </div>
                                            <div class="resource-detail p-3">
                                                <div class="section-description">
                                                    <p class="mb-0"><span><t t-esc="v.title"/></span></p>
                                                    <p class="mb-0"><t t-esc="v.description"/></p>
                                                </div>
                                                 <div class="link-wrapper mb-3 mt-2">
                                                   <a t-att-href="v.url"  class="link-btn" data-bs-toggle="modal" t-att-data-bs-target="'#a'+ str(v.id)">WATCH VIDEO ></a>
                                                 </div>
                                            </div>
                                        </div>
                                    </div>
                                        <!--video popup -->
                                        <div class="modal fade video-modal" t-att-id="'a'+str(v.id)" tabindex="-1" role="dialog" aria-labelledby="videomodal" aria-hidden="true">
                                          <div class="modal-dialog modal-lg" role="document">
                                            <div class="modal-content">
                                              <div class="modal-header">
                                                <h5 class="modal-title" id="exampleModalLabel"><t t-esc="v.title"/></h5>
                                                <button type="button" class="close" data-bs-dismiss="modal" id="close" aria-label="Close">
                                                    <i class="icon-x"></i>
                                                </button>
                                              </div>
                                              <div class="modal-body p-0">
                                                <div class="video-section-wrapper m-0 border-0">
                                                    <div class="video-wrapper">
                                                        <iframe width="100%" height="200" t-att-src="v.url" autoplay="1"  class="myVideoClass" frameborder="0" allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture" allowfullscreen=""></iframe>
                                                    </div>
                                                </div>
                                              </div>
                                            </div>
                                          </div>
                                        </div>
                                    </t>
                                </div>
                            </div>
                        </section>
                        <!-- Educational Resources -->
                        <section class="video-block bg-grey padding-60-y">
                            <div class="container">
                                 <div class="text-center">
                                     <h1  class="resource-section-title"> <span>Educational</span> Downloads</h1>
                                 </div>
                                <div class="col-md-11 m-auto">
                                    <div class="row mt-5">
                                        <t t-foreach="educational" t-as="e">
                                            <div class="col-md-4 mb-3 px-3 px-md-4">
                                                <div class="resource-box h-100 bg-white">
                                                     <div class="images-wrapper m-0 border-0 p-4">
                                                         <img t-attf-src="/web/image/slide.slide/#{e.id}/image_1024" class="img-fluid img-thumbnail oe_slides_grid_thumbnail" alt="a.name"/>
                                                    </div>
                                                    <div class="resource-detail px-3 pb-3">
                                                        <div class="section-description mb-2">
                                                            <p class="text-center"><span><t t-esc="e.name"/></span></p>

                                                        </div>
                                                        <div class="btn-wrapper text-center mb-3">
                                                            <a t-attf-href="/web/content/slide.slide/#{e.id}/binary_content?download=true" class="btn btn-info btn-sm">Download</a>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </t>
                                    </div>
                                </div>
                            </div>
                        </section>
                        <div class="seprator">
                            <div class="red"></div>
                            <div class="black"></div>
                        </div>
                        <!-- Award And Achievements -->
                        <section class="video-block padding-60-y">
                            <div class="container">
                                 <div class="text-center">
                                     <h1  class="resource-section-title"> <span>Awards</span> and Achievements</h1>
                                 </div>
                                  <div class="col-md-11 m-auto">
                                    <div class="row mt-5">
                                        <t t-foreach="awards" t-as="a">
                                             <div class="col-md-4 mb-3 px-3 px-md-4">
                                            <div class="resource-box h-100 bg-white border-0">
                                                 <div class="images-wrapper m-0 border-0">
                                                    <img t-attf-src="/web/image/slide.slide/#{a.id}/image_1024" class="img-fluid img-thumbnail oe_slides_grid_thumbnail shadow-none border-0" alt="a.name"/>
                                                </div>
                                            </div>
                                        </div>
                                        </t>
                                    </div>
                                </div>
                            </div>
                        </section>
                        <section>
                            <div class="container">
                                <div class="col-md-9 border-separator m-auto"></div>
                            </div>
                        </section>
                        <!-- Content library -->
                        <section class="video-block padding-60-y ">
                            <div class="container">
                                <div class="text-center">
                                    <h1  class="resource-section-title"> <span>Content</span> Library</h1>
                                </div>
                                <div class="row mt-5">
                                    <t t-foreach="blog_post" t-as="c">
                                        <div class="col-md-4 mb-3 px-3 px-md-4">
                                            <div class="resource-box content-lib h-100">
                                                 <div class="images-wrapper m-0 border-0 no-shadow">
                                                      <img t-attf-src="/web/image/blog.post/#{c.id}/image" class="img-fluid img-thumbnail oe_slides_grid_thumbnail"/>
                                                </div>
                                                <div class="resource-detail p-3">
                                                    <div class="section-description">
                                                        <p class="mb-0"><span><t t-esc="c.name"/></span></p>
                                                        <p class="mb-0"><t t-esc="c.description_content()"/></p>
                                                    </div>
                                                     <div class="link-wrapper mb-3 mt-2">
                                                       <a t-att-href="c.website_url"  class="link-btn">READ MORE ></a>
                                                     </div>
                                                </div>
                                            </div>
                                        </div>
                                    </t>
                                </div>
                            </div>
                            <div class="js_sale">
                                <div class="oe_website_sale">
                                    <div class ="products_pager form-inline justify-content-center">
                                        <t t-call="website.pager">
                                            <t t-set="_classes">ml-2</t>
                                        </t>
                                    </div>
                                </div>
                            </div>
                         </section>
                    </div>
                </t>
            </t>
        </t>
    </template>
</odoo>