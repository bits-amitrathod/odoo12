<odoo>
    <template id="stockhawk_spreadsheet_view" name="Stockhawk Spreadsheet">
        <t t-name="website.terms_of_sales">
            <t t-call="website.layout">
                <div id="wrap" class="oe_structure oe_empty"/>
                <section class="about-us-section">
                  <t t-name="Spreadsheet">
                      <html>
                          <head>
                              <link href="/stockhawk_spreadsheet/static/src/css/xspreadsheet.css" rel="stylesheet"/>
                              <link href="/stockhawk_spreadsheet/static/src/css/xspreadsheet.css.map" rel="stylesheet"/>
                              <link href="/stockhawk_spreadsheet/static/src/css/xspreadsheet.js.map" rel="stylesheet"/>
                              <script type="text/javascript" src="/stockhawk_spreadsheet/static/src/js/xspreadsheet.js"></script>
                              <script type="text/javascript" src="/stockhawk_spreadsheet/static/src/js/custom_spreadsheet.js"></script>

                              <!--<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css"></link>
                              <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
                              <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.16.0/umd/popper.min.js"></script>-->
                          </head>
                          <body onload='load()'>
                              <div style="position: fixed; right: 0; top: .7em;">
                              </div>
                              <br></br>
                              <center><img src="/stockhawk_spreadsheet/static/src/images/StockHawk_Logo_TM_R1.png" class="img-fluid"/></center>
                              <div class="container">
                                  <center style="color: rgb(50, 61, 73); font-family: &quot;Open Sans&quot;, sans-serif; font-size: 25px;">Rapid Quote</center>
<!--                                <span style="color: rgb(50, 61, 73); font-family: &quot;Open Sans&quot;, sans-serif; font-size: 15px;">-->
<!--                                    SPS is a leader in providing cost-saving solutions for healthcare-->
<!--                                    facilities across the United States who choose to save money without-->
<!--                                    sacrificing quality of care. We accomplish this by providing hospitals.                                    -->
<!--                                </span>-->
                              </div>
                              <div>
                                <button type="button" class="btn btn-primary abc ml" data-toggle="modal" data-target="#help">Help</button>
                              </div>
                              <!--loader-->
                              <span id="loader" class="loader" style="display: none;"></span>
                              <!-- The Modal -->
                                <div class="modal" id="help">
                                    <div class="modal-dialog">
                                        <div class="modal-content">
                                            <!-- Modal Header -->
                                            <div class="modal-header">
                                              <h4 class="modal-title">Rapid Quote Help</h4>
                                            </div>
                                            <!-- Modal body -->
                                            <div class="modal-body">
                                              <div class="container">
                                                <span style="color: rgb(50, 61, 73); font-family: &quot;Open Sans&quot;, sans-serif; font-size: 15px;">
                                                    <p>
                                                        SPS is a leader in providing cost-saving solutions for healthcare
                                                    facilities across the United States who choose to save money without
                                                    sacrificing quality of care. We accomplish this by providing hospitals
                                                    and surgery centers two ways to save on their high-volume surgical
                                                    disposables. We help our customers recover significant budget costs by
                                                    purchasing their un-used, in-date surgical surplus while at the same
                                                    time, providing them additional cost savings and flexibility when
                                                    purchasing the same high volume products they are currently utilizing.
                                                    </p>
                                                  <p>
                                                    SPS is a leader in providing cost-saving solutions for healthcare
                                                    facilities across the United States who choose to save money without
                                                    sacrificing quality of care. We accomplish this by providing hospitals
                                                    and surgery centers two ways to save on their high-volume surgical
                                                    disposables. We help our customers recover significant budget costs by
                                                    purchasing their un-used, in-date surgical surplus while at the same
                                                    time.
                                                  </p><p>
                                                    SPS is a leader in providing cost-saving solutions for healthcare
                                                    facilities across the United States who choose to save money without
                                                    sacrificing quality of care. We accomplish this by providing hospitals
                                                    and surgery centers two ways to save on their high-volume surgical
                                                    disposables. </p>
                                                </span>
                                            </div>
                                            </div>
                                            <!-- Modal footer -->
                                            <div class="modal-footer">
                                                <button type="button" id="helpClose" class="btn btn-primary abc ml" data-dismiss="modal">Close</button>
                                            </div>
                                        </div>
                                    </div>
                                  </div>
                                <div class="modal" id="myModal">
                                    <div class="modal-dialog">
                                        <div class="modal-content">
                                            <!-- Modal Header -->
                                            <div class="modal-header">
                                              <h4 class="modal-title">Rapid Quote</h4>
                                            </div>
                                            <!-- Modal body -->
                                            <div class="modal-body">
                                              You are about to submit your Rapid Quote. Please verify all information is correct. By clicking “Yes” you will be redirected to a loading page while your
                                                submission is processed.
                                            </div>
                                            <!-- Modal footer -->
                                            <div class="modal-footer">
                                                <button type="button" id="myModalOK" class="btn btn-primary abc ml" data-dismiss="modal">Yes</button>
                                                <button type="button" id="myModalClose" class="btn btn-primary abc ml" data-dismiss="modal">Close</button>
                                            </div>
                                        </div>
                                    </div>
                                  </div>
                                <div class="modal" id="errorModal">
                                    <div class="modal-dialog">
                                        <div class="modal-content">
                                            <!-- Modal Header -->
                                            <div class="modal-header">
                                              <h4 class="modal-title">Rapid Quote</h4>
                                            </div>
                                            <!-- Modal body -->
                                            <div class="modal-body" id="errorMessageDisplay">

                                            </div>
                                            <!-- Modal footer -->
                                            <div class="modal-footer">
                                                <button type="button" id="errorModalOK" class="btn btn-primary abc ml" data-dismiss="modal">Ok</button>
                                                <button type="button" id="errorModalClose" class="btn btn-primary abc ml" data-dismiss="modal">Close</button>
                                            </div>
                                        </div>
                                    </div>
                                  </div>
                                <div id="x-spreadsheet-demo"></div>
                          </body>
                      </html>
                  </t>
                </section>
            </t>
        </t>
    </template>
    <template id="stockhawk_spreadsheet_link" name="Link to frontend portal" inherit_id="portal.user_dropdown">
        <xpath expr="//a[@id='o_logout']" position="before">
            <t t-set="user" t-value="request.env['res.users'].sudo().search([('id', '=', request.session.uid)])"/>
                <t t-if="user.partner_id.active and user.partner_id.prioritization and user.partner_id.on_hold != True">
                    <div style= "display: flex; align-items: center;" >
                    <a style="padding: 4px 0px 1px 0px;"  class="menu-rapid-quote-an" href="/spreadsheet/stockhawk_spreadsheet">
                      <img style="width: 90px; margin-left: 8px;" src="/stockhawk_spreadsheet/static/src/images/StockHawk_Logo_TM_R1.png" class="img-fluid "/>
                    </a>
                    <a style="padding: 4px 0px 1px 0px;" href="/spreadsheet/stockhawk_spreadsheet" role="menuitem" class="dropdown-item ">Rapid Quote</a>
                    </div>
                </t>
        </xpath>
    </template>
</odoo>