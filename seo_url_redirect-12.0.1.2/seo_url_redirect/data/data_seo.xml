<odoo>
    <data noupdate="1">
        <!-- <function id="use_server_rewrites" model="ir.default" name="set"
        eval="('website.seo.rewrite.settings', 'use_server_rewrites', True)"/> -->
        <function id="use_suffix" model="ir.default" name="set"
        eval="('website.seo.rewrite.settings', 'use_suffix', True)"/>
        <function id="suffix_product" model="ir.default" name="set"
        eval="('website.seo.rewrite.settings', 'suffix_product', '.htm')"/>
        <function id="pattern_product" model="ir.default" name="set"
        eval="('website.seo.rewrite.settings', 'pattern_product', 'name,id,default_code')"/>
        <function id="pattern_category" model="ir.default" name="set"
        eval="('website.seo.rewrite.settings', 'pattern_category', 'display_name')"/>
    </data>
</odoo>
