<?xml version="1.0" encoding="UTF-8"?>

<templates xml:space="preserve">

    <t t-name="ks_dashboard_theme_view_owl" owl="1">
        <div class="ks_dashboard_theme_view_render">
            <div class="ks_dashboard_theme_input_container" t-on-click="(ev) => this.ks_dashboard_theme_input_container_click(ev)">
                <input class="ks_dashboard_theme_input ks_color_white" type="checkbox" name="White Theme"
                       value="white" t-att-checked="this.props.value=='white'"/>
                    <span class="ks_checkmark"/>
            </div>
            <div class="ks_dashboard_theme_input_container" t-on-click="(ev) => this.ks_dashboard_theme_input_container_click(ev)">
                <input class="ks_dashboard_theme_input ks_color_blue" type="checkbox" name="Blue Theme" value="blue" t-att-checked="this.props.value=='blue'"/>
                  <span class="ks_checkmark"/>
            </div>
            <div class="ks_dashboard_theme_input_container" t-on-click="(ev) => this.ks_dashboard_theme_input_container_click(ev)">
                <input class="ks_dashboard_theme_input ks_color_red" type="checkbox" name="Red Theme" value="red" t-att-checked="this.props.value=='red'"/>
                  <span class="ks_checkmark"/>
            </div>
            <div class="ks_dashboard_theme_input_container" t-on-click="(ev) => this.ks_dashboard_theme_input_container_click(ev)">
                <input class="ks_dashboard_theme_input ks_color_yellow" type="checkbox" name="Yellow Theme"
                       value="yellow" t-att-checked="this.props.value=='yellow'"/>
                  <span class="ks_checkmark"/>
            </div>
            <div class="ks_dashboard_theme_input_container" t-on-click="(ev) => this.ks_dashboard_theme_input_container_click(ev)">
                <input class="ks_dashboard_theme_input ks_color_green" type="checkbox" name="Green Theme"
                       value="green" t-att-checked="this.props.value=='green'"/>
                  <span class="ks_checkmark"/>
            </div>
        </div>
    </t>
</templates>