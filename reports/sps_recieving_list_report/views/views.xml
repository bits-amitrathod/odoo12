<odoo>
    <data>

        <record model="ir.ui.view" id="form_list_sps">
            <field name="name">sps.report.view.list</field>
            <field name="model">stock.picking</field>
            <field name="priority">20</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="sku"/>
                    <field name="p_name"/>
                    <field name="lot_no"/>
                    <field name="exp_date"/>
                    <field name="qty_recieved"/>
                    <field name="uom"/>

                </tree>
            </field>
        </record>

        <record id="view_stock_quantity_history" model="ir.ui.view">
            <field name="name">Purchsase Report</field>
            <field name="model">sps_recieve_popup.view.model</field>
            <field name="arch" type="xml">
                <form string="Choose your date">
                    <group>
                        <group>
                            <field name="compute_at_date" widget="radio"/>
                            <field name="start_date" attrs="{'invisible': [('compute_at_date', '=', 0)]}"/>
                            <field name="end_date" attrs="{'invisible': [('compute_at_date', '=', 0)]}"/>
                        </group>
                    </group>
                    <footer>
                        <button name="open_table" string="View Report" type="object" class="btn-primary"/>
                        <button string="Cancel" class="btn-default" special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>

        <record id="action_stock_quantity_history" model="ir.actions.act_window">
            <field name="name">SPS Recieving List</field>
            <field name="res_model">sps_recieve_popup.view.model</field>
            <field name="view_type">form</field>
            <field name="view_mode">form</field>
            <field name="view_id" ref="view_stock_quantity_history"/>
            <field name="target">new</field>
            <field name="context">{'default_compute_at_date': 0}</field>
        </record>

        <menuitem id="recieve_list_report"
                  action="action_stock_quantity_history"
                  name="SPS Recieving List"
                  parent="purchase.purchase_report"
                  sequence="4"/>
    </data>
</odoo>