<odoo>

    <report id="action_todo_model_report"
            string="RMA Report"
            model="stock.picking"
            report_type="qweb-html"
            file="rma_report.purchase_report"
            name="rma_report.purchase_report"
    />

    <template id="purchase_report">
        <t t-call="web.html_container">
            <t t-call="web.external_layout">
                <page>
                    <div class="oe_structure"/>
                    <div class="row">

                        <center>
                            <b>
                                <h4>Surgical Product Solutions</h4>
                            </b>
                            <b>
                                <h2>RMA Report</h2>
                            </b>
                        </center>

                        <!-- Report page content -->

                            <table class="table table-condensed table-bordered" style="border: 1px solid black;">
                                <tr style="border: 1px solid black;">
                                    <th style="border: 1px solid black;text-align: center;">Customer</th>
                                    <th style="border: 1px solid black;text-align: center;">Created</th>
                                    <th style="border: 1px solid black;text-align: center;">Status</th>
                                    <th style="border: 1px solid black;text-align: center;">Qty</th>
                                    <th style="border: 1px solid black;text-align: center;">Product</th>
                                    <th style="border: 1px solid black;text-align: center;">Type</th>

                                </tr>


                                    <t t-foreach="data" t-as="o">
                                        <tr>

                                             <td align="left" class="col-xs-2" style="border: 1px solid #000;">
                                                <span t-esc="o.customer"/>
                                            </td>

                                            <td  align="left" class="col-xs-2" style="border: 1px solid #000;">
                                                <span t-esc="o.created"/>
                                            </td>

                                            <td align="left" class="col-xs-2" style="border: 1px solid #000;">
                                                <span t-esc="o.status"/>
                                            </td>
                                            <td align="left" class="col-xs-2" style="border: 1px solid #000;">
                                                <span t-esc="o.qty"/>
                                            </td>
                                             <td align="right" class="col-xs-2" style="border: 1px solid #000;">
                                                <span t-esc="o.product"/>
                                            </td>
                                            <td align="center" class="col-xs-2" style="border: 1px solid #000;">
                                                <span t-esc="o.product"/>
                                            </td>
                                            <!--<td align="left" class="col-xs-2" style="border: 1px solid #000;">-->
                                                <!--<span t-esc="o.vend"/>-->
                                            <!--</td>-->

                                        </tr>
                                    </t>

                            </table>
                        </div>

                </page>
            </t>
        </t>
    </template>

</odoo>