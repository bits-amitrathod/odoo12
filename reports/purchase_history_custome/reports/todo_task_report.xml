<odoo>

    <!--<report id="action_todo_model_report"
            string="Purchase History"
            model="purchase.order.line"
            report_type="qweb-html"
            file="purchase_history_custome.purchase_report"
            name="purchase_history_custome.purchase_report"
    />-->

        <record id="action_todo_model_report" model="ir.actions.report">
            <field name="name">Purchase History</field>
            <field name="model">purchase.order.line</field>
            <field name="report_type">qweb-html</field>
            <field name="report_name">purchase_history_custome.purchase_report</field>
            <field name="report_file">purchase_history_custome.purchase_report</field>
            <field name="print_report_name">Purchase History</field>
            <field name="binding_model_id" ref="model_purchase_order_line"/>
            <field name="binding_type">report</field>
        </record>

    <template id="purchase_report">
        <t t-call="web.html_container">
            <t t-call="web.external_layout">
                <page>
                    <div class="oe_structure"/>
                    <div>

                        <center>
                            <b>
                                <h4>Surgical Product Solutions</h4>
                            </b>
                            <b>
                                <h4>Purchase History Report</h4>
                            </b>
                        </center>
                        <center>
                            <h4 t-if="date">
                                <span t-esc="date"/>
                            </h4>
                        </center>
                        <!-- Report page content -->

                            <table class="table table-sm table-bordered" style="border: 1px solid #000;">
                                <thead>
                                <tr>
                                    <th style="border: 1px solid black;text-align: center;">Purchase Order#</th>
                                    <th style="border: 1px solid black;text-align: left;">Product SKU</th>
                                    <!--<th style="border: 1px solid black;text-align: center;">Manufacturer</th>-->
                                    <th style="border: 1px solid black;text-align: center;">Vendor Name</th>
                                    <th style="border: 1px solid black;text-align: center;">Product Name</th>
                                    <th style="border: 1px solid black;text-align: center;">Delivered Qty</th>
                                    <th style="border: 1px solid black;text-align: center;">Product UOM</th>
                                    <th style="border: 1px solid black;text-align: center;">Date Done</th>
                                    <th style="border: 1px solid black;text-align: center;">Min Expiration Date</th>
                                    <th style="border: 1px solid black;text-align: center;">Max Expiration Date</th>
                                    <th style="border: 1px solid black;text-align: center;">Offer Price</th>
                                    <th style="border: 1px solid black;text-align: center;">Retail Price</th>
                                     <th style="border: 1px solid black;text-align: center;">Total Delivered Qty Retail Price</th>
                                    <th style="border: 1px solid black;text-align: center;">Total Delivered Qty Offer Price</th>
                                </tr>
                                </thead>
                                <tbody>
                                <!--<t t-foreach="data.keys()" t-as="user">-->
                                    <t t-foreach="data" t-as="o">
                                        <tr>
                                            <td  style="border: 1px solid #000;">
                                                <span t-esc="o.order_name"/>
                                            </td>
                                            <td  style="border: 1px solid #000;">
                                                <span t-esc="o.sku"/>
                                            </td>
                                            <!--<td  style="border: 1px solid #000;">-->
                                                <!--<span t-esc="o.manufacturer_rep"/>-->
                                            <!--</td>-->
                                            <td  style="border: 1px solid #000;">
                                                <span t-esc="o.vendor"/>
                                            </td>
                                            <td style="border: 1px solid #000;">
                                                <span t-esc="o.product_name"/>
                                            </td>
                                            <td align="right"  style="border: 1px solid #000;">
                                                <span t-esc="o.qty"/>
                                            </td>
                                             <td align="right"  style="border: 1px solid #000;">
                                                <span t-esc="o.product_uom.name"/>
                                            </td>
                                            <td align="right"  style="border: 1px solid #000;">
                                                <span t-esc="o.date_planned"/>
                                            </td>
                                            <td align="center"  style="border: 1px solid #000;">
                                                <span t-esc="o.minExpDate" t-options="{'widget':'date'}"/>
                                            </td>
                                            <td align="center"  style="border: 1px solid #000;">
                                                <span t-esc="o.maxExpDate" t-options="{'widget':'date'}"/>
                                            </td>
                                            <td align="right"  style="border: 1px solid #000; white-space:nowrap;">
                                                <span t-esc="o.product_unit_price" t-options='{"widget": "monetary", "display_currency": o.currency_id}'/>
                                            </td>
                                            <!--<td align="right"  style="border: 1px solid #000; white-space:nowrap;">-->
                                                <!--<span t-esc="o.retail_price" t-options='{"widget": "monetary", "display_currency": o.currency_id}'/>-->
                                            <!--</td>-->
                                              <td align="right"  style="border: 1px solid #000; white-space:nowrap;">
                                                <span t-esc="o.product_offer_price" t-options='{"widget": "monetary", "display_currency": o.currency_id}'/>
                                            </td>
                                            <td align="right"  style="border: 1px solid #000; white-space:nowrap;">
                                                <span t-esc="o.delivered_product_retail_price" t-options='{"widget": "monetary", "display_currency": o.currency_id}'/>
                                            </td>
                                              <td align="right"  style="border: 1px solid #000; white-space:nowrap;">
                                                <span t-esc="o.delivered_product_offer_price" t-options='{"widget": "monetary", "display_currency": o.currency_id}'/>
                                            </td>
                                        </tr>
                                    </t>
                                </tbody>
                                <!--</t>-->
                            </table>
                        </div>

                </page>
            </t>
        </t>
    </template>

</odoo>