<odoo>

    <report id="action_sps_receiving_list_report_pdf"
            string="SPS Receiving List"
            model="stock.move.line"
            report_type="qweb-pdf"
            file="sps_receiving_list_report.adjustment_report"
            name="sps_receiving_list_report.adjustment_report"
    />

    <report id="action_sps_receiving_list_report"
            string="SPS Receiving List"
            model="stock.move.line"
            report_type="qweb-html"
            file="sps_receiving_list_report.adjustment_report"
            name="sps_receiving_list_report.adjustment_report"
    />


    <report id="action_sps_receiving_list_report1"
            string="SPS Receiving List"
            model="stock.picking"
            report_type="qweb-html"
            file="sps_receiving_list_report.adjustment_report1"
            name="sps_receiving_list_report.adjustment_report1"
    />

    <template id="adjustment_report">
        <t t-call="web.html_container">
            <t t-call="web.external_layout">
                <div class="page">
                    <center>
                        <b>
                            <h4>Surgical Product Solutions</h4>
                        </b>
                        <b>
                            <h2>SPS Receiving List</h2>
                        </b>
                    </center>


                    <t t-foreach="new_receiving_list" t-as="new_receiving_list_int">
                             <t t-set="new_receiving_item" t-value="new_receiving_list[new_receiving_list_int]"/>

                        <table class="tav  ble table-condensed" style="border: 1px solid black;width:100%;margin-bottom: 50px;">
                            <thead>
                                <tr>
                                    <th style="border: 1px solid black;text-align:  center !important; width:70%">
                                        Product Name
                                    </th>
                                    <th style="border: 1px solid black;text-align:  center !important; width:10%">
                                        Qty Received
                                    </th>
                                    <th style="border: 1px solid black;text-align:  center !important; width:10%">
                                        Date Validated
                                    </th>
                                    <th style="border: 1px solid black;text-align:  center !important; width:15%">
                                        UOM
                                    </th>
                                </tr>
                            </thead>
                    <tbody>

                      <div class="col-sm-6" style="margin-bottom: 10px;">
                            <strong>To be Received Date :</strong>
                             <span t-esc="context_timestamp(datetime.datetime.now()).strftime('%m/%d/%Y')"/>
                        </div>
                     <div class="col-sm-6" style="margin-bottom: 10px;">
                            <strong>Purchase Order :</strong>
                            <span t-esc="new_receiving_item['po_order_no']"/>
                        </div>
                        <div class="col-sm-6" style="margin-bottom: 10px;">
                            <strong>Vendor :</strong>
                            <span t-esc="new_receiving_item['ven_id']"/>
                        </div>
                    <t t-foreach="new_receiving_item['data']" t-as="receiving_list_int">
                        <t t-set="receiving_item" t-value="new_receiving_item['data'][receiving_list_int]"/>
                              <!--<t t-foreach="receiving_item_list" t-as="receiving_item_int_temp">-->
                                   <!--<t t-set="receiving_item_ls" t-value="receiving_item_list[receiving_item_int_temp]"/>-->


                                <tr>
                                    <td style="border-top: 1px solid black;">
                                        <b>
                                            <span t-esc="receiving_item['product']"/>
                                        </b>
                                    </td>
                                    <td style="border-top: 1px solid black;">

                                    </td>
                                    <td style="border-top: 1px solid black;">

                                    </td>
                                    <td style="border-top: 1px solid black;">

                                    </td>
                                </tr>
                                <tr t-foreach="receiving_item['lots']" t-as="lot">
                                    <td style="border: 1px solid black; text-align:  center ">
                                        Lot# :
                                        <span t-esc="lot['lot_id']"/>
                                        Exp Date :
                                        <span t-esc="lot['exp_date']" t-options='{"widget": "datetime"}'/>
                                    </td>

                                    <td style="border: 1px solid black; text-align:  center ">
                                            <span t-esc="lot['qty_done']"/>
                                    </td>
                                    <td style="border: 1px solid black; text-align:  center ">
                                            <span t-esc="lot['date_done']" t-options='{"widget": "datetime"}'/>
                                    </td>
                                    <td style="border: 1px solid black; text-align:  center ">
                                        <span t-esc="lot['product_uom_id']"/>
                                    </td>
                                </tr>
                                <tr style="height: 25px;">
                                    <td style="border: 1px solid black;">

                                    </td>
                                     <td style="border: 1px solid black;">

                                    </td>
                                     <td style="border: 1px solid black;">

                                    </td>
                                    <td style="border: 1px solid black;">

                                    </td>

                                </tr>
                            <!--</t>-->
                          </t>

                </tbody>

  </table>
                    </t>
                </div>

            </t>
        </t>
    </template>

