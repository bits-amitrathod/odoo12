<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <!-- QWeb Reports -->
        <record id="action_pick_report" model="ir.actions.report">
            <field name="name">Pick Report</field>
            <field name="model">pick_report.popup</field>
            <field name="report_type">qweb-html</field>
            <field name="report_name">pick_report.pick_tmpl</field>
            <field name="report_file">pick_report.pick_tmpl</field>
        </record>

        <record id="action_pick_report_pdf" model="ir.actions.report">
            <field name="name">Pick Report</field>
            <field name="model">pick_report.popup</field>
            <field name="report_type">qweb-pdf</field>
            <field name="report_name">pick_report.pick_tmpl</field>
            <field name="report_file">pick_report.pick_tmpl</field>
            <field name="attachment_use">True</field>
        </record>

    </data>
</odoo>