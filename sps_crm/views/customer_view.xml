<odoo>
    <data>
       <record id="view_form_external_field1" model="ir.ui.view">
            <field name="name">External Fields</field>
            <field name="model">res.partner</field>
            <field name="inherit_id" ref="base.view_partner_form"/>
            <field name="arch" type="xml">
                <xpath expr="/form/sheet/notebook/page[@name='internal_notes']" position="after">
                    <page name="externalfields" string="Details"
                          attrs="{'invisible':['|', '|', ('is_parent', '==', False), ('customer_rank', '==', 0)]}">
<!--                            <field name="partner_id"/>-->
                        <group>
                            <group>

                            <field name="gpo" invisible="1"/>
                            <field name="purchase"/>
                            <field name="mesh"/>
                            <field name="edomechanicals"/>
                            <field name="orthopedic"/>
                            <field name="facilityERP" invisible="1"/>
                            <field name="description"/>
                            <field name="suture" />
                            <field name="gynecological"/>
                            <field name="uology"/>
                            </group>
                            <group>
<!--   According to Client request we Remove this field's From View                             -->
                            <field name="edoscopy"/>
                            <field name="woundcare"/>
                            <field name="bariatric"/>

                            <field name="acc_cust_parent" invisible="1"/>
                            <field name="generalnotes" invisible="1"/>
                            <field name="ent"/>
                            <field name="fiscal_year_end" invisible="1"/>
                            <field name="last_modify_by"/>
                            <field name="created_by"/>

<!--                            <field name="captis"/>-->
<!--                            <field name="illucient"/>-->
<!--                            <field name="capstone_health_aliance"/>-->
<!--                            <field name="salina_contract"/>-->
<!--                            <field name="mha"/>-->
<!--                            <field name="veteran_affairs"/>-->
<!--                            <field name="partners_co_operative"/>-->
<!--                            <field name="magnet_group" />-->
<!--                            <field name="fsasc"/>-->
<!--                            <field name="uspi"/>-->
<!--                            <field name="surgery_partners"/>-->
<!--                            <field name="intalere_contract"/>-->
<!--                            <field name="premier"/>-->
                            <field name="email_opt_out"/>
                            <field name="time_zone" invisible="1"/>
<!--                            <field name="facility_type"/>-->
                            <field name="bed_size" invisible="1"/>
                            <field name="purchase_history_date" invisible="1"/>
<!--                            <field name="acq_account"/>-->
<!--                            <field name="sales_account"/>-->
<!--                            <field name="competitors_id" widget="many2many_tags" invisible="1"/>-->
                            <!--<field name="type"/>-->
                            <field name="status_id" widget="many2many_tags"
                            attrs="{'invisible': ['|', '&amp;', ('is_company','=', True),('parent_id', '=', False),('company_name', '!=', False),('company_name', '!=', '')]}"/>


                            </group>
                        </group>
                    </page>
                    <page name="activitynotesfields" string="Activity Notes"
                          attrs="{'invisible':['|', '|', ('is_parent', '==', False), ('customer_rank', '==', 0)]}">

                        <group>
                                   <field name="sales_activity_notes"/>
                                   <field name="acq_activity_notes"/>
                        </group>
                    </page>
                    <page name="wishlist" string="Wishlist">
                        <field name="wishlist_product_ids">
                            <tree>
                                <field name="name"/>
                                <field name="actual_quantity"/>
                                <field name="qty_available"/>
<!--                                <field name="website_id"/>-->
                            </tree>
                        </field>
                    </page>
                </xpath>
                <field name="vat" position="attributes">
                    <attribute name="groups">account.group_account_manager</attribute>
                </field>
                <field name="vat" position="before">
                    <field name="acc_cust_parent" />
                </field>
                <field name="vat" position="after">
                    <field name="ordering_day1" widget="many2many_tags" />
                    <field name="time_zone" />
                    <field name="bed_size" />
                    <field name="gpo" />
                    <field name="facilityERP"/>
                    <field name="top_subspecialties1" widget="many2many_tags"/>
                    <field name="competitors_id" widget="many2many_tags"/>
                    <field name="purchase_history_date"/>
                    <field name="generalnotes" class="text-break"/>
                </field>
                <field name="website" position="after">
                    <field name="fiscal_year_end" />
                </field>
                <xpath expr="//field[@name='category_id']" position="after">
                    <field name="status_id" widget="many2many_tags"
                    attrs="{'invisible': ['|', '&amp;', ('is_company','=', True),('parent_id', '=', False),('company_name', '!=', False),('company_name', '!=', '')]}"
                    />
            </xpath>
            <xpath expr="/form/sheet/notebook/page[@name='contact_addresses']/field/form/sheet/group/group[1]" position="inside">
                    <field name="status_id" widget="many2many_tags"/>
            </xpath>
            <xpath expr="//div[hasclass('oe_kanban_details')][last()]" position="inside">
                    <field name="status_id" widget="many2many_tags"/>
<!--                    <div t-if="record.status_id">
                        <li t-foreach="record.status_id" t-as="item">
                            <t t-esc="item.name"/>
                        </li>
                    </div>-->
            </xpath>
            </field>
        </record>

